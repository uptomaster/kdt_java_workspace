package review;
// 1번 : 복습
public class Review {

	public static void main(String[] args) {
			
		//printf() : 서식 지정 출력메소드
		System.out.printf("'%s' '%d' '%f' '%s'","Hello", 2025, 6.17, true );
		System.out.println(); //줄바꿈
		System.out.printf("'%7s' '%d' '%f' '%s'","Hello", 2025, 6.17, true );
		// 문자열이 5자리인데, 7자리로 서식지정을 하니 공백이 추가되었음.
		System.out.printf("'%7s' '%5d' '%f' '%s'","Hello", 2025, 6.17, true );
	}

}
