package homework08;

// 5번: A ~ F까지를 배열에 값을 넣고 출력
public class ArrayTask05 {
	public static void main(String[] args) {

		// 알파벳 6개를 담을 문자형 타입의 배열 선언 (6칸)
		char []arr = new char[6];
		
		// 반복문으로 A ~ F를 각 인덱스에 맞게 넣어준다.
		
		
		
		for(char i = 'A'; i < 'F'; i++) {
			for(int j = 0; j < 6; j++) {
				arr[j] = i;
			}
			
		}
		
		// 출력문 작성(반복문으로 배열값 출력)
		for(int i = 0; i < arr.length; i++) {
			System.out.printf(arr[i] + " ");
		}
		
		
	} // main 종료
}
