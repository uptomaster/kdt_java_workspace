package ifTest;
// 9번 : 조건문(if문)
public class IfTest01 { // 클래스 중괄호 시작
	public static void main(String[] args) { // 메인문 중괄호 시작
		
		System.out.println("출력 시작");
		
		int number = 10;
		
		if(number == 10) { // if문 중괄호 시작
//			System.out.println("if문의 조건식이 true입니다.");
//			System.out.printf("숫자는 %d입니다.\n", number);
			
			// 조건식에서 number에 저장된 값이 10과 같다면
			// number에 10을 더한값을 저장하고 number 변수 출력하기.
//			number += 10;
			
			
		} // if문 중괄호 종료
		System.out.println("number의 값은 : " + number);
		System.out.println("출력 끝");
	} // main 중괄호 종료
}// 클래스 중괄호 종료
