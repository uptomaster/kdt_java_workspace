package switchTest;
// 14번 : switch문
public class SwitchTest01 {

	public static void main(String[] args) {
		
		String animal = "고양이";
		
		switch(animal) {
		case "고양이":
			System.out.println("선택한 동물은 고양이입니다.");
		
		case "강아지":
			System.out.println("선택한 동물은 강아지입니다.");
			
		case "코알라":
			System.out.println("선택한 동물은 코알라입니다.");
		default:
			System.out.println("선택한 동물이 없습니다.");
			break;
			
		}
		
		
		
	}

}
