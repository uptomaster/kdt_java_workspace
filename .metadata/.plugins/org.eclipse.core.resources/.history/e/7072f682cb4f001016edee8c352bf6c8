package review;

public class ArrayTask06 {

	public static void main(String[] args) {
		// 6번 : A ~ F까지 중 C만 제외하고 배열에 담아 출력
		
		// 5칸짜리 문자형 배열 선언
		char [] arr = new char[5];
		// 정수형 임시변수 = 0;
		
		//for문으로 C만 제외하고 배열에 A ~ F 생성해서 담기
		for(int i = 'A'; i <= 'F'; i++) {
			if(i == 'C') {
				continue;
			}
			arr[i-'A'] = (char)i;
		}
		
		for(int i = 0; i< 5; i++) {
			System.out.printf(arr[i] + " ");
		}
		
	}

}
