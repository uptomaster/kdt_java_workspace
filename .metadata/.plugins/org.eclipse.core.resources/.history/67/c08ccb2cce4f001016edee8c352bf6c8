package review;

public class ArrayTask06 {

	public static void main(String[] args) {
		// 6번 : A ~ F까지 중 C만 제외하고 배열에 담아 출력
		
		// 5칸짜리 문자형 배열 선언
		char [] arr = new char[5];
		// 정수형 임시변수 = 0;
		char ch = 'A';
		int temp = 0;
		
		// while문으로 작성하기
		
		while(temp < 5) {
			if(ch != 'C') {
				arr[temp++] = ch;
			}
			ch++;
			System.out.println(arr);
		}
		
	}

}
