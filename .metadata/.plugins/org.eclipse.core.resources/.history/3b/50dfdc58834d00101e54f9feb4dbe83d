package twoForTest;
// 2번 : 2행 3열 출력하기
public class TwoForTest02 {

	public static void main(String[] args) {
		
//		for(int i = 0; i < 2; i++) {
//			for(int j = 0; j < 3; j++) {
//				System.out.printf("i = %d, j = %d\n", i,j);
//			}
//			System.out.println();
//		}


//		***
//		***
//		모양 만들기
		for(int i = 0; i < 2; i++) {
			for(int j = 0; j < 3; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
//		*
//		**
//		*** 출력문제
		
		for(int i = 1; i <= 3; i++) {
			for(int j = 1; j <= i; j++) {
				System.out.printf("*");
			}
			System.out.println();
		}
		
		
		
	}// main 종료
}
