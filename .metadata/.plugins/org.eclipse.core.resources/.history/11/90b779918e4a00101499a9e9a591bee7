package typeCasting;
// 13번 : 모든 데이터 타입 조합
public class TypeCasting03 {

	public static void main(String[] args) {
		
		//변수
		byte b = 10;
		short s = 100;
		char c = 'a'; // a는 유니코드 숫자로 97
		int i = 1000;
		long l = 10000L;
		float f = 3.14f;
		double d = 3.141592;
		boolean bool = true;
		
		// byte -> 
		short bs = b;
//		char ci = b;
		int bi = b;
		long bl = b;
		float bf = b;
		double bd = b;
//		boolean bbool = b;
		
		
	}

}
