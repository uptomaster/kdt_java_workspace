package oper;
// 3번 : 관계연산자와 논리연산자
public class OperTest01 {

	public static void main(String[] args) {
		System.out.println("======================");
		int num1 = 10, num2 = 20, num3 = 10;
		System.out.println(num1 + " > " + num2 + " : " + (num1 > num2));
		System.out.println(num1 + " < " + num2 + " : " + (num1 < num2));
		System.out.println(num1 + " >= " + num2 + " : " + (num1 >= num2));
		System.out.println(num1 + " <= " + num2 + " : " + (num1 <= num2));
		System.out.println(num1 + " == " + num2 + " : " + (num1 == num2));
		System.out.println(num1 + " != " + num2 + " : " + (num1 != num2));
	}

}
