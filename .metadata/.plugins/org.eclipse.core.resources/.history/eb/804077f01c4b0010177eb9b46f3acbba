package typeCasting;
// 3번 : 문자열 자료형 -> 다른 자료형
public class TypeCasting02 {

	public static void main(String[] args) {
		// 기본자료형 -> 문자열 자료형
		String str1 = 3 + "";
		String str2 = "" + 6.17;
		String str3 = 'a' + "";
		String str4 = true + "";
		
		System.out.println(str1);
		System.out.println(str2);
		System.out.println(str3);
		System.out.println(str4);
	}

}
