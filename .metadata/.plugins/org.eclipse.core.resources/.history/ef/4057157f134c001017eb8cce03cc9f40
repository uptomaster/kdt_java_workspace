package ifTest;
// 12번 : if ~ else if ~ else문
public class IfTest03 {

	public static void main(String[] args) {
		
		// 양수, 음수, 0 인지 출력하기
		
		System.out.println("출력 시작");
		
		int num = 10;
		
		if(num > 0) {
			System.out.println("양수입니다.");
		}
		else if(num < 0) {
			System.out.println("음수입니다.");
		}
		else {
			System.out.println("0 입니다.");
		}
		
		
		
		System.out.println("출력 끝");
	}

}
